{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Simple Menu",
	"name": "rcm/simple-menu",
	"category": "widgets",
	"icon": "menu",
	"version": "1.0.0",
	"example": {},
	"supports": {
		"interactivity": true,
		"anchor": true,
		"color": {
			"link": true,
			"text": true
		}
	},
	"textdomain": "rcm-simple-menu",
	"editorScript": "file:./index.ts",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.ts",
	"attributes": {
		"menu": {
			"type": "string",
			"default": ""
		},
		"layout": {
			"type": "string",
			"default": "horizontal"
		},
		"editMode": {
			"type": "boolean",
			"default": false
		}
	}
}
